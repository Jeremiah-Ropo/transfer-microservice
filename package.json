  "description": "transfer-microservice",
  "main": "eslintrc.js",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node ./dist/index.js",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
