    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
